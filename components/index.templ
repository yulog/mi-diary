package components

templ Index(title string, reaction templ.Component, tags templ.Component, users templ.Component) {
	@Base(title) {
		<a href="/settings">settings</a>
		<div>
			<h3>Notes</h3>
			<a href="/notes">All Notes</a>
		</div>
		<div>
			<h3>Reactions</h3>
			@reaction
		</div>
		<div>
			<h3>HashTags</h3>
			@tags
		</div>
		<div>
			<h3>Users</h3>
			@users
		</div>
		<div>
			<h3>Archives</h3>
			<a href="/archives">All Archives</a>
		</div>
	}
}
