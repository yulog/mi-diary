package components

templ Settings(title string) {
	@Base(title) {
		<div>
			@getReaction()
		</div>
	}
}

templ getReaction() {
	<form hx-post="/settings/reactions" hx-swap="none">
		<label>
			ID
			<input name="note-id" type="text"/>
		</label>
		<button>
			Get
		</button>
	</form>
}
